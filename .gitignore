# =======================
# Vite / React .gitignore
# =======================

# Dependency directories
node_modules/
.pnpm/
.yarn/

# Build output
dist/
build/

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
vite-error.log*

# Environment files
.env
.env.local
.env.development
.env.production
.env.test

# IDE / Editor configs
.vscode/
.idea/
*.swp
*.swo
*.bak

# System files
.DS_Store
Thumbs.db

# Local lockfiles (optional, keep only one)
package-lock.json
yarn.lock
pnpm-lock.yaml

# Cache
.cache/
vite.config.timestamp.json
